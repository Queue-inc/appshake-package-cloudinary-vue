{
  "dependencies": {},
  "prompts": [
    {
      "type": "Question",
      "text": "What is Cloudinary cloud name?",
      "key": "cloudName"
    },
    {
      "type": "Question",
      "text": "What is Cloudinary upload preset?",
      "key": "uploadPreset"
    },
    {
      "type": "Question",
      "text": "What is Cloudinary folder name?",
      "key": "folderName"
    },
    {
      "type": "Action",
      "command": "appshake set VUE_APP_CLOUDINARY_NAME $cloudName && appshake set VUE_APP_CLOUDINARY_UPLOAD_PRESET $uploadPreset && appshake set VUE_APP_CLOUDINARY_FOLDER $folderName"
    }
  ]
}
